{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "./src",
    // "lib": ["ESNext"],
    "baseUrl": "./"
  },
  "include": ["src"],
  "exclude": ["node_modules"],
  "references": [{ "path": "../../packages/shared" }]
}
